<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.it6.tws.entity.OrderItem" table="orderitem">
        <id name="itemid" type="java.lang.String">
            <column name="itemid"/>
            <generator class="assigned" />
        </id>
        <property name="pid" type="java.lang.String">
            <column name="pid"/>
        </property>
        <property name="classify1" type="java.lang.String">
            <column name="classify1"/>
        </property>
        <property name="classify2" type="java.lang.String">
            <column name="classify2"/>
        </property>
         <property name="oid" type="java.lang.String">
            <column name="oid"/>
        </property>
        <property name="state">
            <column name="state"/>
        </property>
        

        <property name="subtotal" type="java.lang.Double">
            <column name="subtotal"/>
        </property>
        <property name="pnum">
            <column name="pnum"/>
        </property>
       

       
        

    </class>
</hibernate-mapping>
